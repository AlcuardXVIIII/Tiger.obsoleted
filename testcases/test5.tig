/* define valid recursive types */
let
/* define a list */
type intlist = {hd: int, tl: intlist} 

/* define a tree */
type tree ={key: int, children: treelist}
type treelist = {hd: tree, tl: treelist}

var lis:intlist := intlist { hd=1, tl= nil }
var tre:tree := tree {key=1,children=nil}
var trelist:treelist := treelist {hd=tre,tl=nil}
in
	lis;
        tre;
        trelist
end
